package garageChallenge;

public class Bike extends Vehicle {

	private int engineSizeCC;
	
	
	
	
	
	public Bike(String brand, String type, int numOfWheels, double price,int engineSizeCC) {
		super(brand, type, numOfWheels, price);
		
	}

	public int getEngineSizeCC() {
		return engineSizeCC;
	}

	public void setEngineSizeCC(int engineSizeCC) {
		this.engineSizeCC = engineSizeCC;
	}

	
}
