package co.uk.idestsoft;

public class ArraysExcersises {

	public static void method1() {
	
	int[] my =new int[10];
	
	for(int i=0;i<10;i++) {
		my[i]=i;
		System.out.println(my[i]);
	}
	}
	public static void method2() {
		
		int[] my =new int[10];
		int val=0;
		
		for(int i=0;i<10;i++) {
			my[i]=i;
		}
		for(int i=0;i<my.length;i++) {
			System.out.println(my[i]*10);
			
		}
	
		
		}
}
