package oopExcercises;

public class Person {

	private String name;
	private int age;
	
	
	
	public Person(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	}

	public void eat() {
		System.out.println("My name is " + name);
	}
	
	public void sleep() {
		System.out.println("I'm " + age+" years old");
	}
}
