package co.uk.idestsoft;

public class BlackJack {

	public static int game(int num1,int num2) {

if(num1>21 && num2>21) {
	return 0;
}		
if (num1>21 && num2<=21 && num2>0)
{
	return num2;
}
if (num2>21 && num1<=21 && num1>0)
{
	return num1;
}
if(num1>num2)
{
	return num2;
}
else
{
	return num1;
}

	}
}