package enums;

public enum EnumsExamples {

	MON(1),
	TUE(2),
	WED(3),
	THU(4),
	FRI(5),
	SAT(6),
	SUN(7);
	
	int weekDay=0;
	
	
	
	EnumsExamples(int weekDay){

		this.weekDay=weekDay;
	}



	public int getWeekDay() {
		return weekDay;
	}



	public void setWeekDay(int weekDay) {
		this.weekDay = weekDay;
	}
	
	
	
}
