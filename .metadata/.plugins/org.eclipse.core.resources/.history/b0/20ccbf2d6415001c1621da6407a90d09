package co.uk.idestsoft;

public class BlackJack {

	public static int game(int num1,int card2) {

if(num1>21 && card2>21) {
	return 0;
}		
if (num1>21 && card2<=21 && card2>0)
{
	return card2;
}
if (card2>21 && num1<=21 && num1>0)
{
	return num1;
}
if(num1>card2)
{
	return card2;
}
else
{
	return num1;
}

	}
}